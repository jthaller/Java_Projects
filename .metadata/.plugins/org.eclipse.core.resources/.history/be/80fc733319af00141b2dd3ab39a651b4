public class ShapeComponent extends JComponent {

	public void paintComponent(Graphics g)
	{
		//revover Graphics2D
		Graphics2D g2 = (Graphics2D) g;
		
		Shape shape = new Shape(points,)
		g2.drawShape(shape);
		
		shape.rotate(theta);
		
	}
	
